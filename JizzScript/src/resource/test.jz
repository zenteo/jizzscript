// math package

pack math {
	// A constant is also a variable
	PI = 3.14159264358979323846

	/*
	* A vector is, and will always be, a vector.
	*/
	func Vector {
		// x = $, where the next parameter is $
		// y = $, where the next parameter is $
		$x
		$y
		create = @Vector
		
		func clone {
			return Vector x y
		}
		
		// Vector has a function add(vector)
		func add {
			$v
			if (contains v create) == false {
				x += v
				y += $
				return super
			}
			x += v.x
			y += v.y
			return super
		}
		
		// Vector has a function added(vector)
		func added {
			ret = clone
			ret.add $
			return ret
		}
		
				// Vector has a function add(vector)
		func sub {
			$v
			x -= v.x
			y -= v.y
			return super
		}
		
		// Vector has a function added(vector)
		func subed {
			ret = clone
			ret.sub $
			return ret
		}
		
		func mul {
			$f
			x *= f
			y *= f
			return super
		}
		
		// Vector has a function added(vector)
		func muled {
			ret = clone
			ret.mul $
			return ret
		}
		
		func div {
			$f
			x /= f
			y /= f
			return super
		}
		
		// Vector has a function added(vector)
		func dived {
			ret = clone
			ret.div $
			return ret
		}
		
		// Vector has a function equals(vector)
		func equals {
			$v
			return x == v.x && y == v.y
		}
		
		// Vector has a toString method
		func toString {
			return "[" + x + ", " + y + "]"
		}
	}

}

